
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  password String?
  username String? @unique
  fullName  String?
  description String?
  profileImage String?
  isProfileInfoSet Boolean? @default(false)
  createdAt DateTime @default(now())
}
